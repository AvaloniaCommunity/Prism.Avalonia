<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="SampleMvvmApp.Views.Setting1View">
  <Grid ColumnDefinitions="100,200" RowDefinitions="Auto,Auto,Auto" Margin="10">
    <TextBlock FontWeight="Bold" Text="First Settings" />
    <TextBlock Grid.Column="0" Grid.Row="1" Text="Setting A" />
    <TextBox Grid.Column="1" Grid.Row="1" />
    <TextBlock Grid.Column="0" Grid.Row="2" Text="Setting B" />
    <ComboBox Grid.Column="1" Grid.Row="2" />
  </Grid>
</UserControl>
